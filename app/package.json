{
  "name": "test",
  "version": "1.0.0",
  "homepage": ".",
  "description": "test",
  "scripts": {
    "start": "run-p start:dev",
    "start:dev": "webpack-dev-server --config webpack.config.dev.js --port 3000",
    "test": "jest --watchAll",
    "test:ci": "jest",
    "sass": "node-sass -o css/ css/",
    "clean:build": "rimraf ./build && mkdir build && rm -rf ../resources/*",
    "prebuild": "run-p clean:build",
    "build": "webpack --config webpack.config.prod.js && mv build/* ../resources/"
  },
  "dependencies": {
    "bootstrap": "4.3.1",
    "file-loader": "^6.0.0",
    "immer": "2.1.3",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "prop-types": "15.7.2",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-paginate": "^7.0.0",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-scripts": "^3.4.3",
    "react-toastify": "4.5.2",
    "reselect": "4.0.0",
    "sass-loader": "^9.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-react-app": "7.0.2",
    "cross-env": "^7.0.2",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "^5.15.2",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "0.9.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-fetch": "^2.3.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "4.1.5",
    "onchange": "^3.3.0",
    "postcss-loader": "3.0.0",
    "react-test-renderer": "16.8.4",
    "react-testing-library": "6.0.0",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "react/display-name": "off",
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  },
  "author": "Sajad Hasanain",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}